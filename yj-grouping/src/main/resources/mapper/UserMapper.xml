<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yqf.yjgrouping.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yqf.groupingapi.entity.User">
        <id column="id" property="id"/>
        <result column="nick_name" property="nickName"/>
        <result column="password" property="password"/>
        <result column="avatar_url" property="avatarUrl"/>
        <result column="gender" property="gender"/>
        <result column="province" property="province"/>
        <result column="city" property="city"/>
        <result column="openid" property="openid"/>
        <result column="status" property="status"/>
        <result column="intro" property="intro"/>
        <result column="integral" property="integral"/>
        <result column="last_login_ip" property="lastLoginIp"/>
        <result column="update_time" property="updateTime"/>
        <result column="tag_str" property="tagStr"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, nick_name, password, avatar_url, gender, province, city, openid, status, intro, integral, last_login_ip,
        update_time, tag_str, create_time
    </sql>

</mapper>
